<%= form_for @conn, "/fetch", [as: :fetch], fn f -> %>
  <label>Identifiant</label>
  <%= text_input f, :login %>
  <label>Mot de passe</label>
  <%= password_input f, :password %>
  <label>Date</label>
  <div class="date-wrapper">
    <%= datetime_select f, :date %>
  </div>
  <%= submit "Obtenir" %>
<% end %>
